@page
@model WebServer.Areas.Identity.Pages.Administrator.AdministratorPanelModel

@{
    ViewData["Title"] = "Administrator panel";
}

<h1>@ViewData["Title"]</h1>
<div class="row">
    <div class="col-md-4">
        <section>
            <form id="accounts" method="post">
                <h4>Registered users</h4>
                <hr />
                <div asp-validation-summary="All" class="text-danger"></div>
                <div class="form-group">
                    <button type="submit" class="btn btn-primary">Pull user accounts</button>
                </div>
            </form>
        </section>
    </div>
    <div class="col-md-6 col-md-offset-2">
        <section>
            <h4>Use another service to log in.</h4>
            <hr />
            <partial name="_UnderConstructionPartial" />
        </section>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}

